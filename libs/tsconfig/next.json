{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "NextJS",
  "extends": "./strictest.json",
  "compilerOptions": {
    "jsx": "preserve",
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
